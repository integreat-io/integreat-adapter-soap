{
  "name": "integreat-adapter-soap",
  "version": "0.3.5",
  "description": "SOAP adapter for Integreat",
  "main": "index.js",
  "author": "Kjell-Morten Bratsberg Thorsen <post@kjellmorten.no>",
  "license": "ISC",
  "scripts": {
    "start": "node index.js",
    "test": "nyc --reporter=text-summary ava",
    "test:inspect": "node --inspect node_modules/ava/profile.js",
    "test:watch": "ava --watch",
    "dev": "npm run test:watch",
    "prebuild": "npm run lint && npm run test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "nyc:report": "nyc report",
    "lint": "standard"
  },
  "ava": {
    "files": [
      "lib/**/*-test.js",
      "tests/**/*-test.js",
      "tests/helpers/*.js"
    ],
    "sources": [
      "lib/**/!(*-test).js",
      "index.js"
    ],
    "helpers": [
      "**/helpers/**/*"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*-test.js",
      "tests/helpers/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/integreat-io/integreat-adapter-soap.git"
  },
  "bugs": {
    "url": "https://github.com/integreat-io/integreat-adapter-soap/issues"
  },
  "homepage": "https://github.com/integreat-io/integreat-adapter-soap#readme",
  "engines": {
    "node": ">= 8.6",
    "npm": ">= 5.4"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "dot-prop": "^5.0.0",
    "ramda": "^0.26.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "ava": "^2.0.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.3",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "standard": "^13.0.1"
  }
}
