{
  "name": "integreat-adapter-soap",
  "version": "0.5.0",
  "description": "SOAP adapter for Integreat",
  "main": "index.js",
  "author": "Kjell-Morten Bratsberg Thorsen <post@kjellmorten.no>",
  "license": "ISC",
  "scripts": {
    "start": "node index.js",
    "test": "npm run lint && nyc --reporter=text-summary ava",
    "test:inspect": "node --inspect node_modules/ava/profile.js",
    "test:watch": "ava --watch",
    "dev": "npm run test:watch",
    "prebuild": "npm run lint && npm run test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "nyc:report": "nyc report",
    "lint": "standard"
  },
  "ava": {
    "babel": true,
    "files": [
      "lib/**/*-test.js",
      "tests/**/*-test.js"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*-test.js",
      "tests/helpers/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/integreat-io/integreat-adapter-soap.git"
  },
  "bugs": {
    "url": "https://github.com/integreat-io/integreat-adapter-soap/issues"
  },
  "homepage": "https://github.com/integreat-io/integreat-adapter-soap#readme",
  "engines": {
    "node": ">= 8.6",
    "npm": ">= 5.4"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "dot-prop": "^5.2.0",
    "ramda": "^0.27.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "ava": "^3.10.1",
    "coveralls": "^3.1.0",
    "nock": "^13.0.2",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "standard": "^14.3.4"
  }
}
